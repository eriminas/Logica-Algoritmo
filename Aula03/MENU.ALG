algoritmo "Menu"
VAR
   opcao: inteiro
inicio
   repita
      escreval ("Cadastro de clientes")
      escreval ("1-Incluir")
      escreval ("2-Alterar")
      escreval ("3-Consultar")
      escreval ("4-Excluir")
      escreval ("5-Sair")
      leia(opcao)
   ate opcao = 5
fimalgoritmo